{
  "name": "vue-auth-cognito",
  "version": "0.1.4",
  "description": "A Vue.js Vuex module for authentication using Amazon AWS Cognito",
  "main": "lib/index.js",
  "scripts": {
    "test": "tap -Rspec --coverage --coverage-report=html test/unit/index.js",
    "test:watch": "tape-watch test/unit/index.js -p tap-spec",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "coverage": "tap --cov --coverage-report=html --strict test/unit/index.js"
  },
  "repository": "https://github.com/kopterio/vue-auth-cognito",
  "author": "Evgeny Zislis <evgeny.zislis@gmail.com>",
  "contributors": [
    {
      "name": "Anton Sekatski",
      "email": "antonsekatski@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "amazon-cognito-identity-js": "^1.16.0",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "bower": "^1.8.0",
    "codeclimate-test-reporter": "^0.4.1",
    "coveralls": "^2.12.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.7",
    "tap": "^10.3.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-watch": "^2.2.4",
    "vue": "^2.2.2"
  },
  "release-script": {
    "defaultDryRun": "true",
    "bowerRepo": "git@github.com:kopterio/vue-auth-cognito-bower.git"
  }
}
